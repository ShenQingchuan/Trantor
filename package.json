{
  "name": "trantor",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@10.14.0",
  "scripts": {
    "dev": "run-p dev:client dev:server",
    "dev:client": "vite",
    "dev:server": "tsx watch --env-file=.env.local src/server/main.ts",
    "serve": "run-p serve:client serve:server-build serve:server-run",
    "serve:client": "vite build --watch",
    "serve:server-run": "env $(cat .env.local) nodemon --config nodemon.json",
    "serve:server-build": "pnpm tsc -b ./tsconfig.server.json --watch",
    "build": "run-p build:client build:server",
    "build:vite": "vite build",
    "build:client": "NODE_ENV=production run-p type-check build:vite",
    "build:server": "pnpm tsc -b ./tsconfig.server.json",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test:e2e": "playwright test",
    "type-check": "vue-vine-tsc --build",
    "lint": "eslint . --fix"
  },
  "dependencies": {
    "@hono/mcp": "^0.1.1",
    "@hono/node-server": "^1.17.1",
    "@modelcontextprotocol/sdk": "^1.17.0",
    "@nrsk/unindent": "^1.1.0",
    "@pinia/colada": "^0.17.1",
    "@shikijs/markdown-it": "^3.9.2",
    "@vueuse/core": "^13.5.0",
    "@vueuse/motion": "^3.0.3",
    "consola": "^3.4.2",
    "date-fns": "^4.1.0",
    "front-matter": "^4.0.2",
    "hono": "^4.8.9",
    "lodash-es": "^4.17.21",
    "lru-cache": "^11.1.0",
    "markdown-it": "^14.1.0",
    "nanoid": "^5.1.5",
    "ofetch": "^1.4.1",
    "openai": "^5",
    "pinia": "^3.0.3",
    "pinia-plugin-persistedstate": "^4.4.1",
    "prosekit": "^0.14.2",
    "prosemirror-view": "^1.40.1",
    "ts-retry": "^4.2.5",
    "vue": "^3.5.17",
    "vue-i18n": "^11.1.3",
    "vue-router": "^4.5.1",
    "vue-vine": "1.7.3",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.0.0",
    "@iconify/json": "^2.2.310",
    "@playwright/test": "^1.52.0",
    "@tsconfig/node22": "^22.0.2",
    "@types/jsdom": "^21.1.7",
    "@types/lodash-es": "^4.17.12",
    "@types/markdown-it": "^14.1.2",
    "@types/node": "^24.1.0",
    "@unocss/reset": "^66.4.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/eslint-plugin": "^1.3.4",
    "@vue-vine/eslint-config": "^1.1.8",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "dotenv": "^17.2.1",
    "eslint": "^9.32.0",
    "eslint-plugin-playwright": "^2.2.1",
    "jiti": "^2.5.1",
    "jsdom": "^26.1.0",
    "nodemon": "^3.1.10",
    "npm-run-all": "^4.1.5",
    "npm-run-all2": "^8.0.4",
    "sass": "^1.89.2",
    "tsx": "^4.20.3",
    "typescript": "~5.8.3",
    "unocss": "^66.4.1",
    "unplugin-auto-import": "^19.3.0",
    "vite": "^7.1.0",
    "vite-plugin-inspect": "^11.3.2",
    "vite-plugin-mkcert": "^1.17.8",
    "vite-plugin-vue-devtools": "^8.0.0",
    "vitest": "^3.2.4",
    "vue-vine-tsc": "1.7.3"
  },
  "pnpm": {
    "overrides": {
      "@vue/language-core": "3.0.1"
    }
  }
}
